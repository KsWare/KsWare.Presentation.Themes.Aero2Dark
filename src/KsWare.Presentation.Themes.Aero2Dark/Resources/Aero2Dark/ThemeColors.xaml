	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionaryEx Name="Aero2Dark.ThemeColors.BaseColorResources">
			<Color           x:Key="Aero2Dark.Colors.GrayFF" >#FFFFFF</Color>
			<Color           x:Key="Aero2Dark.Colors.GrayF1" >#FAFAFA</Color> <!-- ControlText -->
			<Color           x:Key="Aero2Dark.Colors.GrayF3" >#F3F3F3</Color>
			<Color           x:Key="Aero2Dark.Colors.GrayD6" >#D6D6D6</Color> <!-- MenuText -->

			<Color           x:Key="Aero2Dark.Colors.GrayD3" >#D3D3D3</Color>
			<Color           x:Key="Aero2Dark.Colors.GrayC4" >#C4C4C4</Color>
			<Color           x:Key="Aero2Dark.Colors.GrayB5" >#B5B5B5</Color>
			<Color           x:Key="Aero2Dark.Colors.GrayA6" >#A6A6A6</Color>
