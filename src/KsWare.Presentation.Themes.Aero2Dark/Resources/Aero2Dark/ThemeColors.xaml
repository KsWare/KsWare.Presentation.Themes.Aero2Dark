			<Color           x:Key="Aero2Dark.Colors.Gray80" >#808080</Color>
			<Color           x:Key="Aero2Dark.Colors.Gray5C" >#5C5C5C</Color> <!-- Disabled Foreground -->
			<Color           x:Key="Aero2Dark.Colors.Gray5B" >#5B5B5B</Color>
			<Color           x:Key="Aero2Dark.Colors.Gray3D" >#3D3D3D</Color> <!-- MenuHighlight -->
