			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ControlBrush"                        Color="{StaticResource Aero2Dark.ThemeColors.ControlColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ControlDarkBrush"                    Color="{StaticResource Aero2Dark.ThemeColors.ControlDarkColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ControlDarkDarkBrush"                Color="{StaticResource Aero2Dark.ThemeColors.ControlDarkDarkColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ControlLightBrush"                   Color="{StaticResource Aero2Dark.ThemeColors.ControlLightColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ControlLightLightBrush"              Color="{StaticResource Aero2Dark.ThemeColors.ControlLightLightColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ControlTextBrush"                    Color="{StaticResource Aero2Dark.ThemeColors.ControlTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.DesktopBrush"                        Color="{StaticResource Aero2Dark.ThemeColors.DesktopColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.GradientActiveCaptionBrush"          Color="{StaticResource Aero2Dark.ThemeColors.GradientActiveCaptionColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.GradientInactiveCaptionBrush"        Color="{StaticResource Aero2Dark.ThemeColors.GradientInactiveCaptionColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.GrayTextBrush"                       Color="{StaticResource Aero2Dark.ThemeColors.GrayTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.HighlightBrush"                      Color="{StaticResource Aero2Dark.ThemeColors.HighlightColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.HighlightTextBrush"                  Color="{StaticResource Aero2Dark.ThemeColors.HighlightTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.HotTrackBrush"                       Color="{StaticResource Aero2Dark.ThemeColors.HotTrackColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InactiveBorderBrush"                 Color="{StaticResource Aero2Dark.ThemeColors.InactiveBorderColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InactiveCaptionBrush"                Color="{StaticResource Aero2Dark.ThemeColors.InactiveCaptionColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InactiveCaptionTextBrush"            Color="{StaticResource Aero2Dark.ThemeColors.InactiveCaptionTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InactiveSelectionHighlightBrush"     Color="{StaticResource Aero2Dark.ThemeColors.InactiveSelectionHighlightColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InactiveSelectionHighlightTextBrush" Color="{StaticResource Aero2Dark.ThemeColors.InactiveSelectionHighlightTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InfoBrush"                           Color="{StaticResource Aero2Dark.ThemeColors.InfoColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.InfoTextBrush"                       Color="{StaticResource Aero2Dark.ThemeColors.InfoTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.MenuBarBrush"                        Color="{StaticResource Aero2Dark.ThemeColors.MenuBarColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.MenuBrush"                           Color="{StaticResource Aero2Dark.ThemeColors.MenuColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.MenuHighlightBrush"                  Color="{StaticResource Aero2Dark.ThemeColors.MenuHighlightColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.MenuTextBrush"                       Color="{StaticResource Aero2Dark.ThemeColors.MenuTextColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ScrollBarBrush"                      Color="{StaticResource Aero2Dark.ThemeColors.ScrollBarColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.WindowBrush"                         Color="{StaticResource Aero2Dark.ThemeColors.WindowColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.WindowFrameBrush"                    Color="{StaticResource Aero2Dark.ThemeColors.WindowFrameColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.WindowTextBrush"                     Color="{StaticResource Aero2Dark.ThemeColors.WindowTextColor}"/>

			<SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}"      Color="Blue"/>
			<SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}"  Color="White"/>
		</ResourceDictionaryEx>
		<LookupResourceDictionary ResourceKey="Aero2Dark.ThemeColors.Brushes.Overrides"/>

		<MergedResourceDictionary Source="/KsWare.Presentation.Themes.Aero2Dark;component\Resources\Aero2Dark\ThemeFonts.xaml"></MergedResourceDictionary>
