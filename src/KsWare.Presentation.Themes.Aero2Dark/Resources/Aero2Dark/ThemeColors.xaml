			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ActiveCaptionBrush"                  Color="{StaticResource Aero2Dark.ThemeColors.ActiveCaptionColor}"/>
			<SolidColorBrush x:Key="Aero2Dark.ThemeColors.ActiveCaptionTextBrush"              Color="{StaticResource Aero2Dark.ThemeColors.ActiveCaptionTextColor}"/>
