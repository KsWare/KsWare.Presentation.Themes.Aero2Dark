		<ResourceDictionaryEx Name="Aero2Dark.ThemeColors.ColorResources">
			<!-- SystemColors.XXX  ==>  Aero2Dark.ThemeColors.XXX -->
			<Color           x:Key="Aero2Dark.ThemeColors.ActiveBorderColor"                   >#1F3F80  </Color> <!-- Rahmen des aktiven Fensters.-->
			<Color           x:Key="Aero2Dark.ThemeColors.ActiveCaptionColor"                  >#1F3F80  </Color> <!-- #FF99B4D1 Hintergrund der Titelleiste des aktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.ActiveCaptionTextColor"              >#FFFFFF  </Color> <!-- #FF000000 Text auf der Titelleiste des aktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.AppWorkspaceColor"                   >#FFABABAB</Color> <!-- #FFABABAB Anwendungsarbeitsbereich                                 -->
			<Color           x:Key="Aero2Dark.ThemeColors.ControlColor"                        >#1F1F1F  </Color> <!-- #FFF0F0F0 Vorderseite eines dreidimensionalen Anzeigeelements -->
			<Color           x:Key="Aero2Dark.ThemeColors.ControlDarkColor"                    >#FFA0A0A0</Color> <!-- #FFA0A0A0 Schatten eines dreidimensionalen Anzeigeelements -->
			<Color           x:Key="Aero2Dark.ThemeColors.ControlDarkDarkColor"                >#FF696969</Color> <!-- #FF696969 dunkler Schatten eines dreidimensionalen Anzeigeelements -->
			<Color           x:Key="Aero2Dark.ThemeColors.ControlLightColor"                   >#FFE3E3E3</Color> <!-- #FFE3E3E3 heller Bereich eines dreidimensionalen Anzeigeelements -->
			<Color           x:Key="Aero2Dark.ThemeColors.ControlLightLightColor"              >#FFFFFFFF</Color> <!-- #FFFFFFFF Hervorhebung eines dreidimensionalen Anzeigeelements -->
			<Color           x:Key="Aero2Dark.ThemeColors.ControlTextColor"                    >#FAFAFA  </Color> <!-- #FF000000 Text in einem dreidimensionalen Anzeigeelement -->
			<Color           x:Key="Aero2Dark.ThemeColors.DesktopColor"                        >#FF1F2345</Color> <!-- #FF1F2345 Desktop -->
			<Color           x:Key="Aero2Dark.ThemeColors.GradientActiveCaptionColor"          >#1F3F80  </Color> <!-- #FFB9D1EA rechten Seite im Farbverlauf der Titelleiste eines aktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.GradientInactiveCaptionColor"        >#1F3F80  </Color> <!-- #FFD7E4F2 rechten Seite im Farbverlauf der Titelleiste eines inaktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.GrayTextColor"                       >#5C5C5C  </Color> <!-- #FF6D6D6D deaktivierter Text -->
			<Color           x:Key="Aero2Dark.ThemeColors.HighlightColor"                      >#3D3D3D  </Color> <!-- #FF3399FF Hintergrund für ausgewählte Elemente -->
			<Color           x:Key="Aero2Dark.ThemeColors.HighlightTextColor"                  >#FAFAFA  </Color> <!-- #FFFFFFFF Text ausgewählter Elemente -->
			<Color           x:Key="Aero2Dark.ThemeColors.HotTrackColor"                       >#FF0066CC</Color> <!-- #FF0066CC vorselektierten Element -->
			<Color           x:Key="Aero2Dark.ThemeColors.InactiveBorderColor"                 >#3D123D  </Color> <!-- #FFF4F7FC Rahmens eines inaktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.InactiveCaptionColor"                >#3D123D  </Color> <!-- #FFBFCDDB Hintergrund der Titelleiste eines inaktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.InactiveCaptionTextColor"            >#808080  </Color> <!-- #FF000000 Text in der Titelleiste eines inaktiven Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.InactiveSelectionHighlightColor"     >#3D3D3D  </Color> <!-- #FFF0F0F0 Hintergrund eines inaktiven ausgewählten Elements -->
			<Color           x:Key="Aero2Dark.ThemeColors.InactiveSelectionHighlightTextColor" >#FAFAFA  </Color> <!-- #FF000000 Texts eines inaktiven ausgewählten Elements -->
			<Color           x:Key="Aero2Dark.ThemeColors.InfoColor"                           >#1F1F1F  </Color> <!-- #FFFFFFE1 Hintergrund des ToolTip-Steuerelements -->
			<Color           x:Key="Aero2Dark.ThemeColors.InfoTextColor"                       >#FAFAFA  </Color> <!-- #FF000000 Text in einem ToolTip-Steuerelement -->
			<Color           x:Key="Aero2Dark.ThemeColors.MenuBarColor"                        >#1F1F1F  </Color> <!-- #FFF0F0F0 Hintergrund einer Menüleiste -->
			<Color           x:Key="Aero2Dark.ThemeColors.MenuColor"                           >#2E2E2E  </Color> <!-- #FFF0F0F0 Hintergrund eines Menüs -->
			<Color           x:Key="Aero2Dark.ThemeColors.MenuHighlightColor"                  >#3D3D3D  </Color> <!-- #FF3399FF markiertes Menüelement -->
			<Color           x:Key="Aero2Dark.ThemeColors.MenuTextColor"                       >#D6D6D6  </Color> <!-- #FF000000 Text eines Menüs -->
			<Color           x:Key="Aero2Dark.ThemeColors.ScrollBarColor"                      >#2E2E2E  </Color> <!-- #FFC8C8C8 Hintergrund einer Schiebeleiste -->
			<Color           x:Key="Aero2Dark.ThemeColors.WindowColor"                         >#1F1F1F  </Color> <!-- #FFFFFFFF Hintergrund des Clientbereichs eines Fensters -->
			<Color           x:Key="Aero2Dark.ThemeColors.WindowFrameColor"                    >#FF646464</Color> <!-- #FF646464 Fensterrahmens -->
			<Color           x:Key="Aero2Dark.ThemeColors.WindowTextColor"                     >#FAFAFA  </Color> <!-- #FF000000 Text im Clientbereich eines Fensters -->
		</ResourceDictionaryEx>
		<LookupResourceDictionary ResourceKey="Aero2Dark.ThemeColors.Colors.Override"/>

		<ResourceDictionaryEx Name="Aero2Dark.ThemeColors.BrushResources">
